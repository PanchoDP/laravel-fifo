{
    "name": "panchodp/laravel-fifo",
    "description": "Simple FIFO inventory system",
    "keywords": ["php","fifo","laravel", "inventory", "management"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "LaravelFifo\\": "src/"
        }
    },
  "autoload-dev": {
    "psr-4": {
      "LaravelFifo\\Test\\": "tests",
      "Database\\Factories\\": "tests/Database/Factories/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelFifo\\Providers\\LaravelFifoServiceProvider"
      ]
    }
  },
    "authors": [
        {
            "name": "Francisco De Pablo",
            "email": "fcodepablo@gmail.com"
        }
    ],
  "require": {
    "php": "^8.4.0"
  },
  "require-dev": {
    "orchestra/testbench": "^10.6",
    "spatie/ray": "^1.42",
    "laravel/pint": "^1.24",
    "larastan/larastan": "^3.6.1",
    "pestphp/pest": "^4.0.3",
    "rector/rector": "^2.1"
  },
  "scripts": {
    "test": [
      "./vendor/bin/pest"
    ],
    "pint": [
      "./vendor/bin/pint"
    ],
    "analyse": [
      "./vendor/bin/phpstan analyse"
    ],
    "dry": [
       "./vendor/bin/rector process --dry-run"
    ],
    "test:all": [
      "@test","@pint","@analyse","@dry"
    ]
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
